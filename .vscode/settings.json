{
    "files.exclude": {
        ".dccache": true,
        ".venv": true,
        "**/__pycache__": true,
        "**/.history": true,
        "**/.mypy_cache": true,
        "**/.pytest_cache": true,
        "**/.ruff_cache": true,
        "**/*.egg-info": true,
        "**/*.pyc": true,
        "**/*.pyd": true,
        "**/*.pyo": true,
        "**/build": true,
        "**/dist": true,
        "bin": true,
        "GeoLite2 Databases": true,
        "images": true,
        "resources": true,
        "Session_Sniffer.exe": true,
        "setup": true,
        "TTS": true
    },
    "python.defaultInterpreterPath": "./.venv/Scripts/python.exe",
    "python.terminal.activateEnvironment": true,
    "python.terminal.activateEnvInCurrentTerminal": true,
    "search.useIgnoreFiles": false
}