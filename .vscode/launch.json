{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Session Sniffer",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/session_sniffer.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "python": "${workspaceFolder}/.venv/Scripts/python.exe",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        }
    ]
}
